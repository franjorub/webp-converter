{
  "name": "webp-converter",
  "version": "1.0.0",
  "description": "This is an open source webp converter",
  "scripts": {
    "start:client": "cd webp-converter-client && npm run dev",
    "start:server": "cd webp-converter-server && npm run dev",
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\""
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
